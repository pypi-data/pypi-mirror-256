{# Jinja opening raw tag would go here if using this as a cookiecutter #}
{# {% load compress static %} #}
<!DOCTYPE html>
<html lang="en-us" dir="ltr">
<head>
{% include "pfe/base_inc/ana_ga4.dtl" %}

<meta charset="utf-8">
<meta name="viewport" content="width=device-width, initial-scale=1">

<title>{% block title %}{% endblock %} | {{PROJ_NAME}}</title>

{% comment "" %}
{% compress css %}
<link rel="stylesheet" href="{% static 'django_table_sort.css' %}"/>

<link rel="stylesheet" href="{% static 'pfe/css/base.css' %}" />
<link rel="stylesheet" href="{% static 'pfe/css/dark_mode.css' %}" />
<link rel="stylesheet" href="{% static 'pfe/css/headers_more.css' %}" />
{% endcompress %}

<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.1.2/css/all.min.css" integrity="sha512-1sCRPdkRXhBV2PBLUdRb4tMg1w2YPf37qatUFeS7zlBy7jJI8Lf4VHwWfZZfpXtYSLy85pkm9GaYVYMfw5BC1A==" crossorigin="anonymous" referrerpolicy="no-referrer" />
{% endcomment %}
    <style>
    {% include "pfe/base_inc/internal_css/1_proto.css" %} {##}
    {% include "pfe/base_inc/internal_css/anchors.css" %} {##}
    {% include "pfe/base_inc/internal_css/images.css" %} {##}
    {% include "pfe/base_inc/internal_css/tables.css" %} {##}
    </style>

    <link rel="icon" href="https://7fp.gitlab.io/01-static-2xod3/icons/largebluediamond.svg" />
</head>

<body>
{% comment "body for media" %}
This does not seem to do anything:  
<body data-media-url="{% get_media_prefix %}">
{% endcomment %}

<header>
    <div id='header-inner'>
    {# {% include "pfe/base_inc/header.dtl" %} #}
        <noscript>Warning: JavaScript is disabled in your browser; some features may not work.</noscript>
        {# <h1 id="site-name"></h1> #}
        <a href="{% url 'site_index' %}">{{ PROJ_NAME }}</a>
    </div>
</header>

<main>
    <div id='main-inner'>
    {# https://code.djangoproject.com/ticket/9173 #}
    {# {% block section_header %}{% endblock %} #}

    {# <div id="content-start" class="content" tabindex="-1"> #}
    {% block content %}{% endblock %}
    </div>
</main>

<footer>
    <div id='footer-inner'>

        {# {% if not user.is_authenticated %}<a href="{% url "login" %}">Login</a>{% endif %} #}

        {% if user.is_authenticated %}
        <a href="{% url "profile" %}">Hi, {{request.user.username}}</a> |
        <a href="{% url "logout" %}">Logout</a>
        {% else %}
        <a href="{% url "login" %}">Login</a>
        {% endif %}

        {% if request.user.is_superuser %}
        | <a href="{% url "admin:index" %}">admin</a> |
        <a href="{% url "mgt_inspect" %}">inspect</a> |
        <a href="{% url "mgt_experiments" %}">exp</a>
        {% endif %}

    <ul>
        {% if request.user.is_superuser %}
        <li>Faves: <a href="{% url 'artwork_list' %}">Art</a></li>
        <li>Faves: <a href="{% url 'collab_list' %}">Collaborators</a></li>
        {% endif %}

    </ul>
    </div>
</footer>

{# {% include "pfe/base_inc/svg.dtl" %} #}

{% comment "" %}
{% compress js %}
<script src="{% static 'pfe/js/theme.js' %}"></script>
{% endcompress %}
{% endcomment %}
</body>
</html>
